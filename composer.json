        "relay/relay": "^1.0",
        "laminas/laminas-httphandlerrunner": "^1.5.0 | ^2.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0|^6.0|^7.0|^8.0|^9.0",
        "php-coveralls/php-coveralls": "^2.0",
        "mockery/mockery": "^1.1"
    },
    "autoload": {
        "psr-4": {
